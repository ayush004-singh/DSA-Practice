class Solution {
    public int first(int[] arr , int t ){
        int low=0;
        int high = arr.length-1;
        int res =-1;
        while(low<=high){
            int mid = high+low/2;
            if(arr[mid]<t){
                low=mid+1;
            }
            else if(arr[mid]>t){
                high=mid-1;
            }
            else{
                res = mid;
                high=mid-1;
            }
        }
         return res;
    }

     public int last(int[] arr , int t ){
        int low=0;
        int high = arr.length-1;
        int res =-1;
        while(low<=high){
            int mid = high+low/2;
            if(arr[mid]<t){
                low=mid+1;
            }
            else if(arr[mid]>t){
                high=mid-1;
            }
            else{
                res = mid;
                low=mid+1;
            }
        }
         return res;
    }
    public int[] searchRange(int[] nums, int target) {
        int f = first(nums,target);
        int l = last(nums,target);

        return new int[]{f,l};
    }
}
